{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "extends": ["../biome.json"],
  "organizeImports": {
    "enabled": false
  },
  "linter": {
    "rules": {
      "suspicious": {
        "noExplicitAny": "off"
      },
      "complexity": {
        "noForEach": "warn"
      }
    }
  },
  "files": {
    "include": ["src/**/*.ts", "src/**/*.tsx"]
  }
}
